[user]
	name = Darwin D Wu
	email = wuddarwin@gmail.com
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[push]
	default = simple
[color]
	ui = true
[alias]
	co = checkout
[include]
	path = ~/gitignore/github_cred
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
